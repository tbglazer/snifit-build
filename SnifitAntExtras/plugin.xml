     <antTask library="lib/antextras.jar" name="snifit.projectSetImport" 
     	headless="true" class="il.co.fibi.snifit.etools.ant.extras.ProjectSetImport" />
     <antTask library="lib/antextras.jar" name="snifit.setDebugInfo" 
     	headless="true" class="il.co.fibi.snifit.etools.ant.extras.SetDebugInfo" />
     <antTask library="lib/antextras.jar" name="snifit.targetRuntimeCreate" 
     	headless="true" class="il.co.fibi.snifit.etools.ant.extras.TargetRuntimeCreate" />
     <antTask library="lib/antextras.jar" name="snifit.workspaceBuild" 
     	headless="true" class="il.co.fibi.snifit.etools.ant.extras.WorkspaceBuild" />
