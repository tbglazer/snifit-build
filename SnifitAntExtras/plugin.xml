     <antTask library="lib/antextras.jar" name="snifit.getProjectData" 
     	headless="true" class="il.co.fibi.snifit.etools.ant.extras.GetProjectData" />
     <antTask library="lib/antextras.jar" name="snifit.productDebug" 
     	headless="true" class="il.co.fibi.snifit.etools.ant.extras.ProductDebug" />
     <antTask library="lib/antextras.jar" name="snifit.projectBuild" 
     	headless="true" class="il.co.fibi.snifit.etools.ant.extras.ProjectBuild" />
