<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="il.co.fibi.snifit.ant.tasks"
         name="%tasksName"
         point="org.eclipse.ant.core.antTasks">
